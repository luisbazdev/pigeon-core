import { IncomingMessage, ServerResponse } from "node:http";
{{#if (eq env "dev")}}
import { createHandler } from "../handler"
{{else}}
import { createHandler } from "pigeon-core"
{{/if}}
/**
 * This is a generated handler for "/api/{{name}}s" route,
 * all request to such will be handled by the handler below.
 */
export const {{name}}Handler = createHandler("/{{name}}s");

{{#each methods}}
{{../name}}Handler.{{this}}("/", async (request: IncomingMessage, response: ServerResponse) => {
  // ...
  response.end()
});
{{/each}}
